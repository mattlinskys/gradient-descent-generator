{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES2015"],
    "module": "commonjs",
    "rootDir": "./src",
    "baseUrl": "./src",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  }
}
